{
  "name": "psq-site",
  "version": "2.0.0",
  "description": "Murderous Sudokus",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf /dist",
    "mklib": "cd src/lib && tsc",
    "mkapp": "cd src/app && tsc",
    "packapp": "cd src/app && webpack --config app.config.js",
    "mksrv": "cd src/server && tsc",
    "mktest": "cd src/spec && tsc",
    "build": "npm run mklib && npm run mksrv && npm run mkapp",
    "test": "npm run mktest && cd src/dist/spec && jasmine *.spec.js"
  },
  "keywords": [
    "killer",
    "sudoku",
    "samurai"
  ],
